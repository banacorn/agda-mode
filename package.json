{
  "name": "agda-mode",
  "main": "./lib/js/src/AgdaMode.bs",
  "version": "0.9.0",
  "description": "agda-mode on Atom",
  "scripts": {
    "start": "bsb -clean && bsb -make-world -w",
    "build": "bsb -make-world && npx webpack --mode=production",
    "test": "apm test"
  },
  "activationCommands": {
    "atom-text-editor": [
      "agda-mode:load"
    ],
    "atom-text-editor.agda": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ],
    "atom-text-editor[data-grammar~='agda']": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ]
  },
  "atomTestRunner": "./lib/js/src/TestRunner.bs.js",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "devDependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@glennsl/rebase": "github:glennsl/rebase",
    "atom": "~/reason/bs-atom",
    "atom-mocha-test-runner": "github:banacorn/atom-mocha-test-runner#6a932a",
    "bs-chai": "^0.5.0",
    "bs-mocha": "^0.5.0",
    "bs-platform": "^5.0.4",
    "bs-webapi": "^0.13.6",
    "chai": "^4.2.0",
    "git-branch": "^2.0.1",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "compare-versions": "^3.5.0",
    "react": "^16.8",
    "react-dom": "^16.8",
    "reason-react": ">=0.7.0",
    "reason-react-update": "^0.1.1"
  }
}

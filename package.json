{
  "name": "agda-mode",
  "main": "./lib/js/src/Main.bs",
  "version": "0.9.10",
  "description": "agda-mode on Atom",
  "scripts": {
    "keymap-gen": "./keymap.el generate",
    "keymap-print": "./keymap.el print",
    "start": "bsb -clean && bsb -make-world -w",
    "build": "bsb -make-world && npx webpack --mode=production",
    "test": "atom -t lib/js/test",
    "build:dev": "npx bsb -make-world && npx lessc style/style.less dist/style.css && webpack --mode development",
    "build:prod": "npx bsb -make-world && npx lessc style/style.less dist/style.css && webpack --mode production"
  },
  "activationCommands": {
    "atom-text-editor": [
      "agda-mode:load"
    ],
    "atom-text-editor.agda": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ],
    "atom-text-editor[data-grammar~='agda']": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ]
  },
  "atomTestRunner": "atom-mocha",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "devDependencies": {
    "less-loader": "^5.0.0",
    "less-watch-compiler": "^1.14.6",
    "vscode-test": "^1.2.2",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@glennsl/bs-json": "github:banacorn/bs-json#0c900d3",
    "agda-mode-vscode": "github:banacorn/agda-mode-vscode#9d13e8d",
    "bs-atom2": "^1.40.1",
    "bs-nd": "^0.1.5",
    "bs-let": "^0.1.11",
    "bs-platform": "^7.2",
    "bs-webapi": "^0.15.3",
    "compare-versions": "^3.5.1",
    "eventemitter3": "^4.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "reason-promise": "^1.0.1",
    "reason-react": ">=0.7.0",
    "reason-react-update": "^0.1.1"
  }
}
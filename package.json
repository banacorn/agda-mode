{
  "name": "agda-mode",
  "main": "./lib/js/src/AgdaMode.bs",
  "version": "0.8.10",
  "description": "agda-mode on Atom",
  "scripts": {
    "start": "bsb -make-world -w",
    "clean": "rm -rf lib",
    "build": "bsb -make-world && npx webpack --mode=production",
    "rebuild": "npm run clean && npm run build"
  },
  "activationCommands": {
    "atom-text-editor": [
      "agda-mode:load"
    ],
    "atom-text-editor.agda": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ],
    "atom-text-editor[data-grammar~='agda']": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ]
  },
  "atomTestRunner": "atom-mocha",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "devDependencies": {
    "@glennsl/bs-json": "^3.0.0",
    "@glennsl/rebase": "github:glennsl/rebase",
    "atom": "github:banacorn/bs-atom#38604b",
    "bs-platform": "^5.0.2",
    "bs-webapi": "^0.13.6",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "react": "^16.8",
    "react-dom": "^16.8",
    "reason-react": ">=0.7.0",
    "reason-react-update": "^0.1.1",
    "semver": "^5.5"
  }
}

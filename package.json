{
  "name": "agda-mode",
  "main": "./lib/js/bundled",
  "version": "0.8.5",
  "description": "agda-mode on Atom",
  "scripts": {
    "start": "bsb -make-world -w",
    "build": "tsc && bsb -make-world && npx webpack",
    "rebuild": "rm -rf lib && npm run build"
  },
  "activationCommands": {
    "atom-text-editor": [
      "agda-mode:load"
    ],
    "atom-text-editor.agda": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ],
    "atom-text-editor[data-grammar~='agda']": [
      "agda-mode:input-symbol",
      "agda-mode:query-symbol",
      "agda-mode:go-to-definition"
    ]
  },
  "atomTestRunner": "atom-mocha",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "devDependencies": {
    "bs-platform": "^4.0.7",
    "ts-loader": "^5.3.0",
    "typescript": "^2.9.2",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2",
    "@glennsl/rebase": "github:glennsl/rebase",
    "@glennsl/bs-json": "^3.0.0",
    "atom": "github:banacorn/bs-atom",
    "bs-webapi": "^0.13.0"
  },
  "dependencies": {
    "@types/atom": "^1.28",
    "@types/bluebird": "^3.5",
    "@types/classnames": "^2.2",
    "@types/lodash": "^4.14",
    "@types/node": "^10.5.1",
    "@types/parsimmon": "^1.10.0",
    "@types/react": "^16.4",
    "@types/react-dom": "^16.0",
    "@types/react-redux": "^6.0.3",
    "@types/redux": "^3.6",
    "@types/redux-actions": "^2.3",
    "@types/semver": "^5.5",
    "bluebird": "^3.5",
    "classnames": "^2.2",
    "duplexer": "^0.1",
    "lodash": "^4.17",
    "parsimmon": "^1.11",
    "react": "^16.4",
    "react-dom": "^16.4",
    "react-redux": "^5.0",
    "reason-react": ">=0.4.0",
    "redux": "^4",
    "redux-actions": "^2.4",
    "redux-thunk": "^2.3.0",
    "semver": "^5.5"
  }
}

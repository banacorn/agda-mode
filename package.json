{
  "name": "agda-mode",
  "main": "./lib/agda-mode",
  "version": "0.6.9",
  "description": "agda-mode on Atom",
  "activationCommands": {
    "atom-text-editor[data-grammar='source agda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ],
    "atom-text-editor[data-grammar='source lagda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ]
  },
  "atomTestRunner": "./lib/test/setup",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">0.180.0"
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "classnames": "^2.2.5",
    "lodash": "^4.17.3",
    "parsimmon": "^0.9.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.11.0",
    "semver": "^5.1.1"
  },
  "devDependencies": {
    "atom-mocha": "^1.0.0",
    "atom-mocha-test-runner": "^0.3.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "chai-things": "^0.2.0",
    "mocha": "^2.3.4",
    "temp": "^0.8.3"
  }
}

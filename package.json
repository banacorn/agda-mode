{
  "name": "agda-mode",
  "main": "./lib/agda-mode",
  "version": "0.7.7",
  "description": "agda-mode on Atom",
  "activationCommands": {
    "atom-text-editor[data-grammar~='agda']": [
      "agda-mode:load",
      "agda-mode:input-symbol"
    ]
  },
  "atomTestRunner": "atom-mocha",
  "repository": "https://github.com/banacorn/agda-mode",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0 <2.0.0"
  },
  "dependencies": {
    "@types/atom": "^1.21.5",
    "@types/bluebird": "^3.5.17",
    "@types/classnames": "^2.2",
    "@types/lodash": "^4.14",
    "@types/node": "^8.0",
    "@types/react": "^16.0",
    "@types/react-dom": "^16.0",
    "@types/react-redux": "^4.4",
    "@types/redux": "^3.6",
    "@types/redux-actions": "^1.2",
    "@types/semver": "^5.3",
    "bluebird": "^3.5",
    "classnames": "^2.2",
    "duplexer": "^0.1",
    "lodash": "^4.17",
    "parsimmon": "^1.6",
    "react": "^16.0",
    "react-dom": "^16.0",
    "react-redux": "^5.0",
    "redux": "^3.7",
    "redux-actions": "^2.2",
    "redux-thunk": "^2.2",
    "semver": "^5.3"
  }
}
